public class HelloJava {

	/**
	 * @param args
	 */
	int arr[] = {1,2,3,4,5};
	private static String say="我要学会你!";
	public static void main(String[] args) {
		int arr[] = {1,2,3,4,5};
		System.out.println("Hello Java "+say);
		for(int x:arr)
		{
			System.out.println(arr[x-1]+'\t');
		}
	}

}
